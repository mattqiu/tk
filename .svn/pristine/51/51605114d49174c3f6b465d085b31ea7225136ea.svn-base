	}
	.nTab .TabTitle ul{
	margin:0;
	padding:0;
	}
	.nTab .TabTitle li{
	float: left;
	width: 393px;
	cursor: pointer;
	height:77px;

	
	list-style-type: none;
	}
	.nTab .TabTitle .active{filter:alpha(opacity=100);  
      -moz-opacity:1.0;  
      -khtml-opacity: 1.0;  
      opacity: 1.0; }
	.nTab .TabTitle .normal{filter:alpha(opacity=60);  
      -moz-opacity:0.6;  
      -khtml-opacity: 0.6;  
      opacity: 0.6;  }
	.nTab .TabContent{
	width:auto;background:#fff;
	margin: 0px auto;
	}
</style>
</head>

