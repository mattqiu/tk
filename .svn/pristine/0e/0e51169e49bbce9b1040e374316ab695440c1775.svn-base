/*小图标*/
@font-face {font-family: iconfont ;
src: url('../front/iconfont.eot?t=1468997863'); /* IE9*/
src: url('../front/iconfont.eot?t=1468997863#iefix') format('embedded-opentype'), /* IE6-IE8 */
url('../front/iconfont.woff?t=1468997863') format('woff'), /* chrome, firefox */
url('../front/iconfont.ttf?t=1468997863') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
url('../front/iconfont.svg?t=1468997863#iconfont') format('svg'); /* iOS 4.1- */}

.iconfont { font-family: iconfont !important; font-size: 16px; font-style: normal; -webkit-font-smoothing: antialiased; -webkit-text-stroke-width: 0.2px; -moz-osx-font-smoothing: grayscale; }
/*小图标end*/
.f-b { min-height: calc(100vh - 50px);}
.b-nav { line-height: 30px; text-align: center; }
.b-nav a { padding: 0 1em; }
a:link, a:visited { text-decoration: none; vertical-align: baseline; color: #333; -webkit-transition: color .3s, background-color .3s; transition: color .3s, background-color .3s; }
a:focus, a:hover, a:active { text-decoration: none; }
.t-c { text-align: center; }
.fs-18 { font-size: 18px; }
.login { background: #fff; }
.login .btn.btn-link { padding: 0; }
.login ul.nav  { position: relative; z-index: 3; display: block; -webkit-box-shadow: 0 2px 4px 0 rgba(0,0,0,.25); -moz-box-shadow: 0 2px 4px 0 rgba(0,0,0,.25); box-shadow: 0 2px 4px 0 rgba(0,0,0,.25); }
.login ul.nav li {  float: left; width: 50%; }
.login ul.nav li a { color: #aaa; background: #333157; font-size: 1.5rem; }
.login ul.nav li a:before { position: absolute; bottom: 0; left: 0; width: 100%; height: 3px; overflow: hidden; background-color: #fff; display: none; content: ""; }
.nav > li > a { padding: 15px; }
.login ul.nav li a:hover, .login ul.nav li.active a { background: #333157; color: #fff; }
.login ul.nav li.active a:before { display: block; }
.input-group { min-height: 4rem; }
.login .form-control { margin-top: .55rem; }
.login .input-group:first-of-type { border-top: 1px solid #ddd; }
.login .input-group-addon, .login .form-control { padding: .5rem .9375rem; line-height: 2rem; background: #fff; border: none; -webkit-box-shadow: none; box-shadow: none; }
.login .btn-primary { border: none; width: 100%; padding: 10px 0; background: #333157; letter-spacing: .5em; }
.login .disabled { background: #666; color: #eee; }
.login .em-1 { text-indent: 1em; }
.login i.iconfont { font-size: 1em; }
.nav-pills > li > a { border-radius: 0; }
.download { margin-top: 30px; }
.download .fs-18 { margin: 10px 0; }
.download i.iconfont { font-size: 1.25em; padding-right: 1rem; }
s.y { display: inline-block; background: #ffb143; height: 1rem; width: 1rem; border-radius: 50%; margin-right: .6rem; }
.app-xz { width: 100%; display: block; background: #fff; padding: 20px 0; font-size: 2.5em; border: 1px solid #ddd; margin: 10px 0; }
.success i { font-size: 17rem; color: #faaf19; display: block; margin-bottom: 0; }
.success p { margin: 3.5rem 0 7.5rem 0; }
.success p .cg { font-size: 2.5rem; color: #333158; font-weight: 600; }
.success a { padding: .75rem 0; font-size: 2rem; color: #fff; width: 100%; display: block; text-align: center; background: #333158; letter-spacing: .5em; border-radius: .4rem; }
.w-100 { width: 100%; }


.container { width: 100% !important; max-width: none !important; }