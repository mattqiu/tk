/*header*/
.header { line-height: 3rem; background: #3e3a39; color: #fff; text-align: center; font-size: 1.2rem; }
.zhifu-tt { margin: 1rem 1rem 0; }
.zhifu-tt p.h { font-size: 1rem; text-align: center; }
.zhifu-tt ul { background: #f5f5f5; padding: 1rem; margin-top: 1rem; }
.zhifu-tt ul li { margin-bottom: 1rem; }
.zhifu-tt ul li * { display: inline-block; }
.zhifu-tt ul li b, .zhifu .radio label b { width: 25%; }
.zhifu-tt ul li em, .zhifu .radio label em { color: #ed7233; }
.zhifu { margin-top: 1rem; font-size: 1.1rem; border-top: 1px solid #e8e8e8; }
.zhifu i { font-size: 7rem; position: relative; top: 2rem; color: #444; }
.zhifu .radio:nth-child(3) i { top: 1rem; font-size: 6rem; }
.zhifu label { display: block; padding-bottom: 1.5rem; }
.zhifu .radio { padding-top: 1.5rem; border-bottom: 1px solid #e8e8e8; }
.zhifu p { margin: 2rem 1rem 1rem; }
button.btn-primary { background: #333157; color: #fff; border-color: #333157; width: 100%; }
button.btn-primary:hover { background: #333157; color: #fff; border-color: #333157; }
.yuan input[type=checkbox]+.text:before, .yuan input[type=radio]+.text:before { border-radius: 50%; font-size: 12px; line-height: 20px; height: 20px; min-width: 20px; position: relative; top: -.2rem; margin-right: 1.5rem; }
.modal-dialog { width: 100%; position: absolute; top: 50%; margin: 0; }
.modal-content { margin: 0 .9375rem; }
.modal-footer { margin-top: -1.5rem; border-top: none;}

/*禁用响应式布局*/
.container { width: 100%; max-width: none !important; }


