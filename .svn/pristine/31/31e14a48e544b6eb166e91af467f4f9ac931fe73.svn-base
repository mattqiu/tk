body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td { margin: 0; padding: 0; }
body, button, input, select, textarea { font: .875rem/1.5 Tahoma, Arial, Helvetica, 'Microsoft YaHei', sans-serif; -ms-overflow-style: scrollbar; color: #333; }
button, input, select, textarea { font-size: 100%; }
address, cite, dfn, em, var { font-style: normal }
button, input, select, textarea, label { vertical-align: middle; outline: none; }
img { border: none; vertical-align: middle; background: url(../img/loading.gif) center no-repeat; max-width: 100% !important; }
ul, li { list-style: none; }
a:link, a:visited { text-decoration: none; vertical-align: baseline; color: #333; }
a:focus{outline:thin dotted;outline:5px auto -webkit-focus-ring-color;outline-offset:-2px}
.clear:after { content: ""; clear: both; display: table; }
.clear { zoom: 1; }/* for IE/Mac */
table { border-collapse: collapse; border-spacing: 0; }
body { word-wrap: break-word; word-break: break-all; _height: 100%; }
/*单项公共样式开始*/
.g-du-3 { -webkit-transition: all .3s ease; -moz-transition: all .3s ease; transition: all .3s ease; }
/*垂直居中*/
.valign { display: table-cell; *display: inline; zoom: 1; vertical-align: middle; }
/*输入框*/
.txt { border-width: 1px; border-style: solid; border-color: #454545 #ccc #ccc #454545; padding-left: 1em; word-wrap: break-word; overflow: hidden; }
.txt:hover, .txt.focus { background-color: #fffcc5; }
/*文字不换行隐藏*/
.text-overflow-hidden { overflow:hidden; text-overflow:ellipsis; -o-text-overflow:ellipsis; white-space:nowrap; }
/*灰色按钮,不能点击*/
.no-btn { background: #f1f1f1; color: #ccc; cursor: default; }
.no-btn:hover { background: none; }
/*规定组内,第一和最后没有属性
:first-of-type { margin-left: none; }
:last-of-type { margin-right: none; }
p:nth-child(2)
*/
