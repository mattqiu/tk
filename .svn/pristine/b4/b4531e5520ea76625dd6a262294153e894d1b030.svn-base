#pagewrap,#pagewrap_footer{
padding: 0px;
width:95%;
margin: 0px auto;
min-width:980px;
_width:expression(document.body.clientWidth < 980 ? "980px" : "auto");

max-width:1190px;
_width:expression(document.body.clientWidth > 1190 ? "1190px" : "auto");
margin:0 auto;
}

.header_bg{height:91px;max-width: 100%;min-width: 980px;background:url(../../img/new/top_bg.png);}
.header_bg .pagewrap_top{
	padding: 0px;
	width:95%;
	margin: 0px auto;
	min-width:980px;
	_width:expression(document.body.clientWidth < 980 ? "980px" : "auto");
	
	max-width:1190px;
	_width:expression(document.body.clientWidth > 1190 ? "1190px" : "auto");
	margin:0 auto;
	height:91px;
}

.header_bg .pagewrap_top #header {height:91px;}
.header_bg .pagewrap_top #header .logo{float:left;width:130px;height:90px;}
.header_bg .pagewrap_top #header .logo img{vertical-align:middle;margin-top:16px;border:0px;}
.header_bg .pagewrap_top #header .text1{width:230px;height:90px;float:left;}
.header_bg .pagewrap_top #header .text1 ul{list-style:none;margin-top:35px;}
.header_bg .pagewrap_top #header .text1 li{text-align:right;font-size:0.75em;color:#4a5765;line-height:20px;width:230px;margin-left:-40px;font-family:Arial, Helvetica, sans-serif;cursor:pointer;}
.header_bg .pagewrap_top #header .cenimg{height:90px;width:140px;float:left;}
.header_bg .pagewrap_top #header .cenimg img{margin-top:10px;border:0px;}
.header_bg .pagewrap_top #header .rigcont{height:90px;width:400px;float:right;}
.header_bg .pagewrap_top #header .rigcont .tpli1{height:40px;width:89%;float:left;margin-top:5px;font-family:Arial, Helvetica, sans-serif;margin-left:5px;}
.header_bg .pagewrap_top #header .rigcont .tpli1 ul{list-style:none;margin-top:0px;height:40px;}
.header_bg .pagewrap_top #header .rigcont .tpli1 li{float:right;line-height:20px;text-align:right;height:25px;margin-top:10px;font-size:0.7em;}
.header_bg .pagewrap_top #header .rigcont .tpli1 li a{color:#999;}
.header_bg .pagewrap_top #header .rigcont .tpli1 li a:hover{color:#426ba7;}

.header_bg .pagewrap_top #header .rigcont .tpli2{height:40px;width:90%;float:left;}
.header_bg .pagewrap_top #header .rigcont .tpli2 ul{list-style:none;margin:8px 0 0 0; padding:0;}
.header_bg .pagewrap_top #header .rigcont .tpli2 li{float:left;width:50%;text-align:right;line-height:20px;font-size:0.75em;}
.header_bg .pagewrap_top #header .rigcont .tpli2 li a{color:#898989;}
.header_bg .pagewrap_top #header .rigcont .tpli2 li a:hover{color:#426ba7;}

#nav{height:41px;max-width: 100%;min-width: 980px;background:url(../../img/new/nav_bg.png);}
#nav #nav_pagewrap
{
	padding: 0px;
	width:95%;
	margin: 0px auto;
	min-width:980px;
	_width:expression(document.body.clientWidth < 980 ? "980px" : "auto");
	
	max-width:1190px;
	_width:expression(document.body.clientWidth > 1190 ? "1190px" : "auto");
	margin:0 auto;
	height:41px;
}

#nav #nav_pagewrap #navul{margin:0 auto;height:26px;padding-top:15px;}

#nav #nav_pagewrap #navul ul, ul li {
	list-style-type:none;
	margin:0;
	padding:0;
	float:left;
}
#nav #nav_pagewrap #navul li {
	float: left;
	margin-right:0px;
	margin-top:-15px;
	padding-top:10px;
	width:150px;
	height:31px;
	position:relative;
	z-index:2000;
	*left:40px;
	text-align:center;
	font-size:0.875em;
}
#nav #nav_pagewrap #navul ul li{margin-left:10px;}

#nav #nav_pagewrap #navul li a:link, #navul li a:visited {
	color:#FFFFFF;
	text-decoration:none;
}
#nav #nav_pagewrap #navul li ul {
	display:none;
	width:260px;
	top:25px;
	left:0px;
	position:absolute;
	z-index:1001;
	left:-50px;
	margin-top:15px;
	border-top:#3c577e 1px solid;
	background:url(../../img/new/menu_bg1.png) 0px 0 no-repeat;
}
#nav #nav_pagewrap #navul li ul li {
	display:block;
	width:210px;
	float:none;
	margin-top:-5px;
	height:35px!important;
	height:45px;
	position:relative;
	*left:0px;
	line-height:30px;
	border-bottom:#3c577e 1px solid;
	font-size:0.875em;
	margin-left:0px!important;
	margin-left:0px;
	text-align:left;
	padding-left:50px;
	text-indent:10px;
}

#nav #nav_pagewrap #navul li.navmoon {
	background:#304769;
	width:150px;
	height:31px!important;
	height:40px;
}
#nav #nav_pagewrap #navul li.navmoon a {
	color:#FFFFFF;
}
#nav #nav_pagewrap #navul li.navhome a:hover {
	color:#FCFF00;
}
#nav #nav_pagewrap #navul li.navmoon ul {
	display:block;
}
#nav #nav_pagewrap #navul li.navmoon ul a {
	display:block;
	width:260px;
	height:28px;
	line-height:30px;
}
#nav #nav_pagewrap #navul li.navmoon ul a:hover {
	border: 0px solid #C0C0C0;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 10px;
	position:relative;
	padding-right:10px;
	/*background:#990;*/
	z-index:2;
	width:auto;
	height:auto;
	/*behavior: url(iecss3.htc);*/
	float:left;
	background:#426ba7;
	text-decoration:none;
}

