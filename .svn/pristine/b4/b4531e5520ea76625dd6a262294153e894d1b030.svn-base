/*js_silder_img_second*/
ol,ul { list-style:none; }

#container {
	width:100%;
	padding:0px;
	margin:0 auto;
	position:relative;
	z-index:0;
}

#slides {
	/*position:absolute;
	top:15px;
	left:4px;
	z-index:100;*/
}


.slides_container {
	width:100%;
	height:400px;
	overflow:hidden;
	position:relative;
}

.pagination {
	margin:-25px auto 0;
	width:120px;
	position:relative;
	z-index:10000;
}

.pagination li {
	float:left;
	margin:0 5px;
}

.pagination li a {
	display:block;
	width:12px;
	height:0;
	padding-top:12px;
	background-image:url(../../img/new/pagination.png);
	background-position:0 0;
	float:left;
	overflow:hidden;
}

.pagination li.current a {
	background-position:0 -13px;
}

/*flags*/
.select_box{float:left;width:90px;font-size:12px;font-family:Arial, Helvetica, sans-serif;}
.open_select_box{width:90px;position:relative;height:18px;}
#ensign{background-image:url(../../img/new/icon_country.png);background-repeat:none;display:inline-block;width:16px;height:11px;position:absolute;left:1px;top:4px;margin:0;}
#open_select{float:left;width:93px;text-align:center;height:18px;line-height:18px;position:relative;cursor:pointer;background:url(../../img/new/select_down_icon.png) no-repeat 68px center;text-align:left;padding-left:21px;}
.select_list{padding:0; position:relative; z-index:10000;height:150px;float:left;width:90px;margin:0;background:#fff;border-top:none;display:none;box-shadow:0 0 4px rgba(0, 0, 0, 0);}
.select_list li{text-align:left;height:15px;cursor:pointer;width:100%;line-height:10px;list-style-type:none;vertical-align:top;position:relative;top:-10px;color:#666;}
.h_bg{background:#e2eaf8;}
.currency_type{display:block;width:100%;position:absolute;left:0;top:0px;text-align:center;font-size:12px;padding-top:3px;}
.currency_icon{background-image:url(../../img/new/icon_country.png);background-repeat:none;display:inline-block;width:16px;height:11px;position:absolute;left:1px;top:7px;}
.usd{background-position:left top;}
.eur{background-position:left -20px;}
.gbp{background-position:left 0px;}
.aud{background-position:left -60px}
.cad{background-position:0 -80px;}
.chf{background-position:0 -100px;}
.hkd{background-position:0 -120px;}
.cny{background-position:0 -140px;}
.select_box a{text-decoration: none;}
.hzd{background-position:0 -160px;}
.cur{background-color: #304769}
