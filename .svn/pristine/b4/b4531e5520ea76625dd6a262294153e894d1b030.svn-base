/*usual_header*/

@font-face {
    font-family: 'UniverLT';
    src:url(../../front/UniverLT.ttf);
}
body {
font-family:'UniverLT','Microsoft YaHei';
src:url(../../front/UniverLT.ttf);
font-size:18px;
background:#f8f8f8;
text-align:center;
margin:0px;
