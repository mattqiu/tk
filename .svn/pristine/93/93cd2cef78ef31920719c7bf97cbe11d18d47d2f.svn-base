.catalog, .menu-cate-all-out { width: 200px; }
.menu-cate-all-out dl dt { padding: 0 10px 0 20px; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; }
.menu-cate-all-out dl.on dt { padding-left: 30px; }
.menu-cate-all-out dd.submenu { left: 200px; }
.menu-cate-all-out dd.submenu li.b-t, .menu-cate-all-out dd.submenu li.n-r { width: auto; display: inline-block; }
.menu-normal li a { padding: 0 15px; }  
.header-cart a.down { width: 160px; }
.catalog h3 { padding-left: 14px; }

.header-bar .nav-top li.w90 .xs { width: 105px; }

