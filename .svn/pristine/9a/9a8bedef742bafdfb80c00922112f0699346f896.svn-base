

.main{
	/*background 			: url('../../../images/bkgd.png') repeat top left;*/
	color 				: white;
	font-weight 		: lighter;
    overflow: auto;
    background-color: silver;
}
/* list stuff */
#org{
	background-color 	: white;
	margin 				: 10px;
	padding 			: 10px;
}

#show-list{
	cursor 				: pointer;
}

table {
    border-collapse: inherit;
    margin:auto;
}
/* bootstrap overrides */
.alert-message{
	margin: 2px 0;
}

.topbar{
	position 			: absolute;
}

/* Custom chart styling */
.jOrgChart {
  margin                : 10px;
  padding               : 20px;
}

/* Custom node styling */
.jOrgChart .node {
	font-size 			: 14px;
	border-radius 		: 8px;
	color 				: #000000;
	-moz-border-radius 	: 8px;
    height   : 80px;
    font-weight: bold;
}
	.node p{
		font-size 		: 10px;
		line-height 	: 11px;
		padding 		: 2px;
	}
#level_span div{
    color: #000000;
    float: left;
    font-weight: bold;
    padding: 5px;
    text-align: center;
    width: 180px;
}
.imgtest{
    margin:0px 0px;
    overflow:hidden;
}

.imgtest div{
    border: 1px solid #fff;
    border-radius: 50px;
    box-shadow: 0 0 3px #ccc;
    height: 51px;
    margin: 0 auto;
    overflow: hidden;
    width: 79px;
}
.node img{
    width:100%;
    min-height:100%;
    text-align:center;
}
.word_break{
    display:block;word-break: break-all;word-wrap: break-word;color:#000000;font-weight: normal;
}
.id_color{
    color: #000000;
}